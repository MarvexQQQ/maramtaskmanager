<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/itembtncall"
        android:layout_width="129dp"
        android:layout_height="137dp"
        android:contextClickable="false"
        android:isScrollContainer="false"
        android:scaleType="centerCrop"
        app:srcCompat="@android:drawable/ic_menu_call"
        tools:layout_editor_absoluteX="176dp"
        tools:layout_editor_absoluteY="240dp" />

    <ImageButton
        android:id="@+id/itembtndel"
        android:layout_width="148dp"
        android:layout_height="133dp"
        android:scaleType="centerCrop"
        app:srcCompat="@android:drawable/ic_menu_delete"
        tools:layout_editor_absoluteX="347dp"
        tools:layout_editor_absoluteY="244dp" />

    <ImageButton
        android:id="@+id/itembtnpen"
        android:layout_width="118dp"
        android:layout_height="133dp"
        android:scaleType="centerCrop"
        app:srcCompat="@android:drawable/ic_menu_edit"
        tools:layout_editor_absoluteX="548dp"
        tools:layout_editor_absoluteY="244dp" />

    <TextView
        android:id="@+id/itemTaskTitle"
        android:layout_width="179dp"
        android:layout_height="111dp"
        android:text="TextView"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="183dp"
        tools:layout_editor_absoluteY="462dp" />

    <TextView
        android:id="@+id/itemTaskSugject"
        android:layout_width="179dp"
        android:layout_height="111dp"
        android:text="TextView"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="442dp"
        tools:layout_editor_absoluteY="462dp" />

    <ImageView
        android:id="@+id/itemimg"
        android:layout_width="195dp"
        android:layout_height="142dp"
        tools:layout_editor_absoluteX="293dp"
        tools:layout_editor_absoluteY="68dp"
        tools:srcCompat="@tools:sample/avatars" />

    <RatingBar
        android:id="@+id/itemstar"
        android:layout_width="335dp"
        android:layout_height="72dp"
        android:progressTint="#070707"
        tools:layout_editor_absoluteX="255dp"
        tools:layout_editor_absoluteY="633dp" />
</androidx.constraintlayout.widget.ConstraintLayout>